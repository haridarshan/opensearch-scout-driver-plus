{
    "name": "haridarshan/opensearch-scout-driver-plus",
    "description": "Extension for OpenSearch Scout Driver",
    "keywords": [
        "scout",
        "opensearch",
        "laravel",
        "driver",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Babenko",
            "email": "babenko.i.a@gmail.com"
        },
        {
            "name": "Hari Darshan Gorana",
            "email": "er.haridarshan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenSearch\\ScoutDriverPlus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSearch\\ScoutDriverPlus\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "haridarshan/opensearch-scout-driver": "dev-master",
        "haridarshan/opensearch-adapter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.5",
        "haridarshan/opensearch-migrations": "dev-master",
        "laravel/scout": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.10",
        "laravel/legacy-factories": "^1.3"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSearch\\ScoutDriverPlus\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
